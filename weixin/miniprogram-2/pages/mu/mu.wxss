/* pages/mu/mu.wxss */
.Pop-up{
    position: fixed;  
    width: 100%;
    height: 100%; 
    background-color: rgba(0, 0, 0, 0.3);
    z-index: 99999;
  }
.Pop-up .cen{
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    width: 600rpx;
    height: 620rpx;
    background-color: #fff;
  }
  .Pop-up .cen .head{
      height: 102rpx;
      text-align: center;
      line-height:102rpx ;
      color: #78be00;
      font-size: 40rpx;
      background-color: #262b3a;
  }
  .Pop-up .cen view:nth-child(2) view{
    float: left;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    align-items: center;
    margin: 22rpx 0 0 33rpx;
  }
  .Pop-up .cen view:nth-child(2) image{
        width: 110rpx;
        height: 110rpx;
  }